{
  "name": "laravel-doctrine/extensions",
  "description": "Doctrine extensions for Doctrine 2 and Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "gedmo",
    "beberlei",
    "laravel",
    "orm",
    "data mapper",
    "database"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/auth": "^9.0",
    "illuminate/config": "^9.0",
    "illuminate/contracts": "^9.0",
    "illuminate/support": "^9.0",
    "illuminate/http": "^9.0",
    "laravel-doctrine/orm": "^2",
    "gedmo/doctrine-extensions": "^3.2",
    "beberlei/doctrineextensions": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "mockery/mockery": "^1.3.1"
  },
  "autoload": {
    "psr-4": {
      "LaravelDoctrine\\Extensions\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
